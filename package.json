{
  "name": "gigabook-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next node_modules package-lock.json && npm cache clean --force",
    "clean:soft": "rm -rf .next && npm cache clean --force",
    "check": "npm run lint && npm run type-check",
    "check:fix": "npm run lint:fix && npm run type-check",
    "check:full": "echo Running lint check... && npm run lint --silent && echo Lint check passed && echo Running type check... && npm run type-check --silent && echo Type check passed && echo Running format check... && npm run format:check --silent && echo Code formatting verified && echo Running build test... && npm run build --silent && echo Production build successful && echo All systems operational",
    "fresh-install": "npm run clean && npm install",
    "build:analyze": "ANALYZE=true npm run build",
    "build:check": "npm run check && npm run build",
    "preview": "npm run build && npm run start",
    "format": "prettier --write . --ignore-path .gitignore",
    "format:check": "prettier --check . --ignore-path .gitignore"
  },
